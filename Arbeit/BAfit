#####
#Functions
f(x)=a*x+b
g(x)=c*x+d
h(x)=e*x+f
#21
tp = (1.869+0.139)**2
tm = (1.869-0.139)**2
t0 = tp*(1-sqrt(1-tm/tp))
zzero(x) = (sqrt(tp-x) - sqrt(tp))/(sqrt(tp-x) + sqrt(tp))
z0(x) = (sqrt(tp-x) - sqrt(tp-t0))/(sqrt(tp-x) + sqrt(tp-t0))
zm(x) = (sqrt(tp-x) - sqrt(tp-tm))/(sqrt(tp-x) + sqrt(tp-tm))
#28
k2_0 = 1/(4*tp)**(5/2)
k1 = sqrt((pi * 1.275**2) / 3)
k2(x) = ( zzero(x) / (-x))**(5/2)
k3(x) = ( z0(x) / (t0-x) )**(-1/2)
k4(x) = ( ( zm(x) ) / (tm-x) )**(-3/4)
k5(x) = (tp-x)/(tp-t0)**(1/4)
#35
phi(x) = k1*k2(x)*k3(x)*k4(x)*k5(x)
phi_0 =k1*k2_0*k3(0)*k4(0)*k5(0)

formsuper(x) = 1/(sqrt((pi * 1.275**2) / 3)*( zzero(x) / (-x))**(5/2)*( z0(x) / (t0-x) )**(-1/2)*( ( zm(x) ) / (tm-x) )**(-3/4)*(tp-x)/(tp-t0)**(1/4)) * (0.071-0.20*z0(x)+0.5*z0(x)**2)

form(x) = 1/phi(x) * (0.071-0.20*z0(x)+0.5*z0(x)**2)
form_0 = 1/phi_0 * (0.071-0.20*z0(0)+0.5*z0(0)**2)


plot k2(x)
print form_0



